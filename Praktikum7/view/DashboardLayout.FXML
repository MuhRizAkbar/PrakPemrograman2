<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="600.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DashboardController">
   <children>
      <TabPane fx:id="mainTabPane" prefHeight="600.0" prefWidth="850.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Data Pelanggan">
             <content>
                <fx:include source="PelangganView.FXML"/>
             </content>
          </Tab>
          <Tab text="Data Buku">
             <content>
                <fx:include source="BukuView.FXML"/>
             </content>
          </Tab>
          <Tab text="Transaksi Penjualan">
             <content>
                <fx:include source="PenjualanView.FXML"/>
             </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>